    <!-- http://www.gong.manchester.ac.uk/odp/owl/List.owl#AnyGeneContainingExon -->

    <owl:Class rdf:about="&List;AnyGeneContainingExon">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&List;GeneStructure"/>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&List;gene_structure_contents"/>
                                <owl:someValuesFrom rdf:resource="&List;Exon"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&List;gene_structure_rest"/>
                                <owl:someValuesFrom>
                                    <owl:Class>
                                        <owl:intersectionOf rdf:parseType="Collection">
                                            <rdf:Description rdf:about="&List;GeneStructure"/>
                                            <owl:Restriction>
                                                <owl:onProperty rdf:resource="&List;gene_structure_contents"/>
                                                <owl:someValuesFrom rdf:resource="&List;Exon"/>
                                            </owl:Restriction>
                                        </owl:intersectionOf>
                                    </owl:Class>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="en">Any Gene that contatins exons</rdfs:comment>
    </owl:Class>
    


