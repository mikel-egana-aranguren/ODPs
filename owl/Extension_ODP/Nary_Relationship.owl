    <!-- http://www.gong.manchester.ac.uk/odp/owl/Extensional_ODP/Nary_Relationship.owl#CFTRGolgiToPlasmaTransport -->

    <owl:Class rdf:about="&Nary_Relationship;CFTRGolgiToPlasmaTransport">
        <rdfs:subClassOf rdf:resource="&Nary_Relationship;NaryRelationshipDomain"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&Nary_Relationship;transports_to"/>
